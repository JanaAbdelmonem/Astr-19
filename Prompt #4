class Jellyfish:
    def __init__(self, arm_length, leg_length, num_eyes, has_tail, is_furry):
        # physical characteristics
        self.arm_length = arm_length    
        self.leg_length = leg_length  
        self.num_eyes = num_eyes    
        self.has_tail = has_tail     
        self.is_furry = is_furry        

    def describe(self):
        print("Animal: Jellyfish")
        print(f"Length of the arms: {self.arm_length}")
        print(f"Length of the legs: {self.leg_length}")
        print(f"Number of eyes: {self.num_eyes}")
        print(f"Does it have a tail? {self.has_tail}")
        print(f"Is it furry? {self.is_furry}")

my_jellyfish = Jellyfish(
    arm_length=4.0,
    leg_length=0.0,
    num_eyes=5,
    has_tail=False,  
    is_furry=False     
)

my_jellyfish.describe()
